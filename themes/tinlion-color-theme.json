  "colors": {
    "activityBar.activeBorder": "#4dffbb",
    "activityBar.background": "#031d16",
    "activityBar.foreground": "#abf3ab",
    "activityBar.inactiveForeground": "#65916595",
    "activityBarBadge.background": "#18b8a2",
    "activityBarBadge.foreground": "#0d3d0d",
    "badge.background": "#21222c",
    "badge.foreground": "#bbd1dd",
    "button.background": "#1bb992",
    "button.foreground": "#2f574d",
    "button.hoverBackground": "#168b6e",
    "dropdown.background": "#1d2021",
    "dropdown.border": "#3c3836",
    "dropdown.foreground": "#ebdbb2",
    "editor.background": "#051F20",
    "editor.foreground": "#50c998",
    "editor.hoverHighlightBackground": "#496b58e8",
    "editor.inactiveSelectionBackground": "#535f5f6b",
    "editor.findMatchHighlightBackground": "#003030",
    "editor.foldBackground": "#00303050",
    "editor.lineHighlightBackground": "#00303050",
    "editor.rangeHighlightBackground": "#002020",
    "editorGroupHeader.tabsBackground": "#163832",
    "editor.selectionBackground": "#214641",
    "editorHoverWidget.background": "#21222c",
    "editorHoverWidget.border": "#63d1bf",
    "editorHoverWidget.foreground": "#bbd1dd",
    "editorIndentGuide.activeBackground1": "#2e4238",
    "editorLineNumber.foreground": "#72A097",
    "editorWidget.background": "#031d16",
    "editorWidget.border": "#214124",
    "editorWidget.foreground": "#3cbb91",
    "focusBorder": "#11f194",
    "gitDecoration.modifiedResourceForeground": "#f7c465",
    "gitDecoration.deletedResourceForeground": "#cc241d",
    "gitDecoration.untrackedResourceForeground": "#98971a",
    "gitDecoration.ignoredResourceForeground": "#7c6f64",
    "gitDecoration.conflictingResourceForeground": "#b16286",
    "input.background": "#031d19",
    "input.foreground": "#bbd1dd",
    "menu.background": "#0B2B26",
    "menu.foreground": "#DAF1DE",
    "menu.selectionBackground": "#235347",
    "menu.selectionForeground": "#8eB69B",
    "menubar.selectionBackground": "#000B07",
    "minimap.selectionHighlight": "#33cc33a4",
    "list.activeSelectionBackground": "#00303050",
    "list.activeSelectionForeground": "#84f0a8",
    "list.dropBackground": "#002828",
    "list.focusBackground": "#00303050",
    "list.highlightForeground": "#91f091",
    "list.hoverBackground": "#00303050",
    "list.inactiveSelectionBackground": "#002020",
    "list.inactiveSelectionForeground": "#aee4ae",
    "peekViewEditor.background": "#124b33",
    "peekViewEditorGutter.background": "#21222c",
    "peekViewEditor.matchHighlightBackground": "#488084",
    "progressBar.background": "#844848",
    "scrollbar.shadow": "#021611",
    "scrollbarSlider.activeBackground": "#354d35",
    "scrollbarSlider.hoverBackground": "#0b4729",
    "scrollbarSlider.background": "#0a352f99",
    "selection.background": "#689d6a80",
    "settings.headerForeground": "#33cc33",
    "settings.modifiedItemIndicator": "#a64dff",
    "sideBar.background": "#0B2B26",
    "sideBar.foreground": "#b9fadc",
    "sideBarTitle.foreground": "#abf5ab",
    "statusBar.background": "#055548",
    "statusBar.debuggingBackground": "#0bbd90",
    "statusBar.foreground": "#ffffff",
    "statusBar.noFolderBackground": "#163832",
    "statusBar.noFolderForeground": "#33cc33",
    "tab.activeBackground": "#051F20",
    "tab.activeForeground": "#4dffbb",
    "tab.inactiveBackground": "#0B2B26",
    "tab.inactiveForeground": "#fdf7fd",
    "tab.unfocusedActiveForeground": "#33cc3395",
    "tab.unfocusedActiveBorder": "#9933ff95",
    "tab.unfocusedInactiveForeground": "#33cc3390",
    "titleBar.activeBackground": "#163832",
  },
  "tokenColors": [
    {
        "scope": ["comment", "punctuation.definition.comment"],
        "settings": {
            "foreground": "#1b7443"
        }
    },
    {
        "scope": ["variable.other.readwrite.alias"],
        "settings": {
            "foreground": "#d5e9257e",
        }
    },
    {
        "scope": ["keyword.control.import", "string.quoted.single", "keyword.control.export"],
        "settings": {
            "foreground": "#4cf59886"
        }
    },
    {
        "scope": ["punctuation.separator.comma", "punctuation.definition.block"],
        "settings": {
            "foreground": "#0cd4ca",
        }
    },
    {
        "scope": ["punctuation.decorator","entity.name.function"],
        "settings": {
            "foreground": "#0eee7e"
        }
    },
    {
        "scope": ["meta.object-literal.key"],
        "settings": {
            "foreground": "#ad95da",
        }
    },
    {
        "scope": ["constant.language.boolean.true"],
        "settings": {
            "foreground": "#9eff02",
            "fontStyle": "italic"
        }
    },
    {
        "scope": ["constant.language.boolean.false"],
        "settings": {
            "foreground": "#ff5f03",
            "fontStyle": "italic strikethrough" 
        }
    },
    {
        "scope": ["variable.other.readwrite"],
        "settings": {
            "foreground": "#fabefa"
        }
    },
    {
      "scope": ["meta.brace.square", "meta.brace.literal", "meta.array.literal"],
      "settings": {
        "foreground": "#af85e6",
      }
    },
    {
        "scope": ["storage.type.class"],
        "settings": {
            "foreground": "#b280f5",
        }
    },
    {
        "scope": ["entity.name.type.class"],
        "settings": {
            "foreground": "#82b1ee",
            "fontStyle": "bold"
        }
    },
    {
        "scope": ["variable.object.property"],
        "settings": {
            "foreground": "#9ac7b0"
        }
    },
    {
        "scope": ["punctuation.definition.string.template.begin", "punctuation.definition.string.template.end", "string.template"],
        "settings": {
            "foreground": "#fabefa"
        }
    },
    {
        "scope": ["constant.numeric.decimal"],
        "settings": {
            "foreground": "#f8bc63",
        }
    },
    {
        "scope": ["punctuation.definition.template-expression.begin", "punctuation.definition.template-expression.end"],
        "settings": {
            "foreground": "#adab21"
        }
    },
    {
      "scope": ["variable.language.this"],
      "settings": {
        "foreground": "#fd7a3e",
      }
    },
    {
        "scope": ["punctuation.accessor", "variable.other.property"],
        "settings": {
            "foreground": "#f7c77f",
        }
    },
    {
        "scope": ["string.quoted.single", "string.quoted.double"],
        "settings": {
            "foreground": "#1fada6",
        }
    },
    // {
    //     "scope": ["entity.name.function"],
    //     "settings": {
    //         "foreground": "#f0d5a4"
    //     }
    // },
    {
      "scope": ["support.type.primitive"],
      "settings": {
        "foreground": "#a9f85e"
      }
    },
    {
      "scope": ["storage.type"],
      "settings": {
        "foreground": "#7fdf1f"
      }
    },
    {
      "scope": ["storage.modifier", "support.constant.property-value.css"],
      "settings": {
        "foreground": "#f2fc6e"
      }
    },
    {
      "scope": ["variable.other.object", "support.variable.property"],
      "settings": {
        "foreground": "#d2f09a"
      }
    },
    {
      "scope": ["keyword.operator.arithmetic", "constant.numeric.css"],
      "settings": {
        "foreground": "#4cf59886"
      }
    },
    {
        "scope": ["entity.other.attribute-name.class.css"],
        "settings": {
            "foreground": "#4cf5e7cb"
        }
    },
    {
        "scope": ["entity.other.attribute-name.pseudo-class.css"],
        "settings": {
            "foreground": "#d0fa9fcb"
        }
    },
    {
        "scope": ["support.type.property-name.css"],
        "settings": {
            "foreground": "#fce6c6cb"
        }
    }
