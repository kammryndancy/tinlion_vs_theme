    "terminal.ansiBrightWhite": "#c4edc4",
    "terminal.foreground": "#9fd3bf",
    "textLink.foreground": "#00ffc8",
    "textLink.activeForeground": "#00ffc8b2",
    "titleBar.activeBackground": "#163832",
    "titleBar.activeForeground": "#e7fffdef",
    "titleBar.inactiveForeground": "#97a7a5cb",
    "titleBar.inactiveBackground": "#13302a",
    "titleBar.border": "#163832",
  },
  "tokenColors": [
    {
        "scope": ["comment", "punctuation.definition.comment"],
        "settings": {
            "foreground": "#1b7443"
        }
    },
    {
        "scope": ["variable.other.readwrite.alias"],
        "settings": {
            "foreground": "#d5e9257e",
        }
    },
    {
        "scope": ["keyword.control.import", "string.quoted.single", "keyword.control.export"],
        "settings": {
            "foreground": "#4cf59886"
        }
    },
    {
        "scope": ["punctuation.separator.comma", "punctuation.definition.block"],
        "settings": {
            "foreground": "#0cd4ca",
        }
    },
    {
        "scope": ["punctuation.decorator","entity.name.function"],
        "settings": {
            "foreground": "#0eee7ea6"
        }
    },
    {
        "scope": ["meta.object-literal.key"],
        "settings": {
            "foreground": "#ad95dac7",
        }
    },
    {
        "scope": ["constant.language.boolean.true"],
        "settings": {
            "foreground": "#02ff02af",
            "fontStyle": "italic"
        }
    },
    {
        "scope": ["constant.language.boolean.false"],
        "settings": {
            "foreground": "#ff5303c5",
            "fontStyle": "italic strikethrough" 
        }
    },
    {
        "scope": ["variable.other.readwrite"],
        "settings": {
            "foreground": "#fabefaaf"
        }
    },
    {
      "scope": ["meta.brace.square", "meta.brace.literal", "meta.array.literal"],
      "settings": {
        "foreground": "#af85e6",
      }
    },
    {
        "scope": ["storage.type.class"],
        "settings": {
            "foreground": "#dfcbfa9a",
        }
    },
    {
        "scope": ["entity.name.type.class"],
        "settings": {
            "foreground": "#fab595c9",
            "fontStyle": "bold"
        }
    },
    {
        "scope": ["variable.object.property"],
        "settings": {
            "foreground": "#9ac7b0e7"
        }
    },
    {
        "scope": ["punctuation.definition.string.template.begin", "punctuation.definition.string.template.end", "string.template"],
        "settings": {
            "foreground": "#fabefa"
        }
    },
    {
        "scope": ["constant.numeric.decimal"],
        "settings": {
            "foreground": "#f8bc63",
        }
    },
    {
        "scope": ["punctuation.definition.template-expression.begin", "punctuation.definition.template-expression.end"],
        "settings": {
            "foreground": "#adab21"
        }
    },
    {
      "scope": ["variable.language.this"],
      "settings": {
        "foreground": "#fd7a3e",
      }
    },
    {
        "scope": ["punctuation.accessor", "variable.other.property"],
        "settings": {
            "foreground": "#f7c77fc5",
        }
    },
    {
        "scope": ["string.quoted.single", "string.quoted.double"],
        "settings": {
            "foreground": "#1fada6",
        }
    },
    {
      "scope": ["support.type.primitive"],
      "settings": {
        "foreground": "#a9f85e9d"
      }
    },
    {
      "scope": ["storage.type", "storage.type.java", "punctuation.definition.annotation.java"],
      "settings": {
        "foreground": "#2de9cacb"
      }
    },
    {
      "scope": ["storage.modifier", "support.constant.property-value.css"],
      "settings": {
        "foreground": "#f2fc6e94"
      }
    },
    {
      "scope": ["variable.other.object", "support.variable.property"],
      "settings": {
        "foreground": "#d2f09aa4"
      }
    },
    {
      "scope": ["keyword.operator.arithmetic", "constant.numeric.css"],
      "settings": {
        "foreground": "#4cf59886"
      }
    },
    {
        "scope": ["entity.other.attribute-name.class.css"],
        "settings": {
            "foreground": "#4cf5e7cb"
        }
    },
    {
        "scope": ["entity.other.attribute-name.pseudo-class.css"],
        "settings": {
            "foreground": "#d0fa9fcb"
        }
    },
    {
        "scope": ["support.type.property-name.css"],
        "settings": {
            "foreground": "#fce6c6cb"
        }
    },
    {
        "scope": ["storage.modifier.import.java"],
        "settings": {
            "foreground": "#4cf5e7cb"
        }
    },
    {
        "scope": ["string.unquoted.plain.out.yaml"],
        "settings": {
            "foreground": "#7cca96fb"
        }
    },
    {
        "scope": ["entity.name.tag", "punctuation.definition.tag"],
        "settings": {
            "foreground": "#4cf5eda4"
        }
    }
  ]
}
